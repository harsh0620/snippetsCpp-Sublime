int getSum(Node *root)
{
    if (root == NULL)
    {
        return 0;
    }
    return getSum(root->left) + getSum(root->right) + root->data;
}
]]></content>
  <tabTrigger>treeSum</tabTrigger>
  <description>Tree Node Sum</description>
